{
  "name": "TabTrace",
  "description": "Visual indicator for your last visited tabs",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "src/js/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/html/popup.html"
  },
  "permissions": ["tabs", "tabGroups", "storage", "windows"]
}